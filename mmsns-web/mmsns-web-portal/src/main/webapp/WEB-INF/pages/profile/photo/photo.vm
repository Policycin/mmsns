<body>
#parse("/portal/header.vm")
<div class="layout-body">
    <div class="layout-left">
        <div class="photo-classify">
            <div class="user-info">
                <span class="user-logo"><a href="/profile/"><img src="https://gw.alicdn.com/bao/uploaded/i1/180558071/TB2sy6jXMQc61BjSZFGXXa1DFXa_!!180558071.jpg_400x400q90.jpg?t=1513063905068" width="100" height="100" style="border-radius: 50%"></a></span>
                <span class="user-name"><a href="/profile/">baby慕慕</a></span>
                <span class="user-words"><a href="/profile/">发布120个帖子</a></span>
                <span class="user-writephoto"><a href="/profile/babymm/photo/upload">上传图库</a></span>
            </div>
            <div class="classify-body">
                <div class="classify-header">
                    <span class="classifytitle" style="line-height: 30px;font-size: 18px;font-weight: 400">分类</span>
                    <span class="classifycount">共6篇</span>
                </div>
                <hr>
                <ul>
                    <li>
                        <a href="/profile/babymm/photo">
                            <span class="classifytitle">我发布的</span>
                            <span class="classifycount">3</span>
                        </a>
                    </li>
                    <li>
                        <a href="/profile/babymm/photo">
                            <span class="classifytitle">我收藏的</span>
                            <span class="classifycount">3</span>
                        </a>
                    </li>
                    <li>
                        <a href="/profile/babymm/photo">
                            <span class="classifytitle">我点赞的</span>
                            <span class="classifycount">3</span>
                        </a>
                    </li>
                </ul>
            </div>
            <div class="classify-body">
                <div class="classify-header">
                    <span class="classifytitle" style="line-height: 30px;font-size: 18px;font-weight: 400">相册</span>
                    <span class="classifycount">共2个</span>
                </div>
                <hr>
                <ul>
                    <li>
                        <a href="/profile/babymm/photo/album">
                            <span class="classifytitle">快乐一家人</span>
                            <span class="classifycount">3</span>
                        </a>
                    </li>
                    <li>
                        <a href="/profile/babymm/photo/album">
                            <span class="classifytitle">柚子宝宝</span>
                            <span class="classifycount">3</span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="layout-right">
        <div class="photo-body">
            <h2 style="font-size: 30px;font-weight: 400;color: green;border-radius: 10%">我的图库</h2>
            <hr>
            <ul>
                #set($photos=[0 .. 2])
                #foreach($photo in $photos)
                    <li>
                        <div class="photocontent">
                            <span class="phototitle">深入区分javascript中的作用域和执行环境</span>
                            <div class="site-demo-flow photoimg">
                                <img src="https://gw.alicdn.com/bao/uploaded/i2/701696736/TB2PNl5ahQa61Bjy0FhXXaalFXa_!!701696736.jpg_400x400q90.jpg?t=1513063905068">
